{
  "manifest_version": 3,
  "name": "SafeWebGuard",
  "version": "1.0",
  "description": "Blocks harmful redirects, rates site safety, and suggests alternatives.",
  "permissions": [
    "webNavigation",
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
